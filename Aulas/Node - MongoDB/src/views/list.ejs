<!DOCTYPE html>
<html lang="en">
    <%- include('partials/head.ejs')%>
    <body>
    
        <h1><%= title%></h1>
        <%- include('partials/navbar.ejs')%>
      
        <ul>
        <% users.forEach((user) => { %>
            <li>
                <%= user.name%> | <%= user.age%> anos | <%= user.email%> - <a href="/edit?id=<%= user._id%>">>> Editar <<</a> - <a href="/remove/<%= user._id%>">>> Apagar <<</a>
            </li>
        <%}) %>
        </ul>
    </body>
</html>